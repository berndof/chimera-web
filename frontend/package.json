  "description": "",
  "dependencies": {
    "@alpinejs/collapse": "^3.14.9",
    "@alpinejs/focus": "^3.14.9",
    "@alpinejs/mask": "^3.14.9",
    "@tailwindcss/cli": "^4.0.14",
    "alpinejs": "^3.14.9",
    "concurrently": "^9.1.2",
    "esbuild": "^0.25.1",
    "lucide": "^0.479.0",
    "tailwindcss": "^4.0.14"
  }
}
